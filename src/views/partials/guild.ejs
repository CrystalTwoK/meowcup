
<div id="guild-button" class="row ripple">
  <div class="col-2">
  
    <% if (guild.icon != null) { %>
        <% if (guild.icon.substring(0,2) === 'a_') { %>
            <img class="guild-icon " src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon%>.gif" alt="<%- guild.name%>"/>
            <% } else { %>
                <img class="guild-icon" src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon%>.png" alt="<%- guild.name%>"/>
                <% } %>
    <% } else { %>

        <img class="guild-icon" src="https://i.imgur.com/bMaLqzT.png" width="120px">
        <% } %>
      </div>
      <div class="col-10">
        <p class="guild-name"><%- guild.name %></p>
        <p class="guild-id"><%- guild.id%></p>
      </div>
    </div>
